{
  "name": "hihome-client",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "cross-env nodemon",
    "dev:debug": "cross-env nodemon --config nodemon-debug.json",
    "lint": "node_modules/.bin/eslint './{src,pages,server,app}/**/*' --ext .js,.jsx,.ts,.tsx",
    "build": "next build && tsc --project server/tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node server/dist/server/index.js",
    "staging": "cross-env NODE_ENV=production PORT=3001 node server/dist/server/index.js",
    "lint-fix": "node_modules/.bin/eslint './{src,pages,server,app}/**/*' --ext .tsx,.ts --fix",
    "storybook": "start-storybook -s ./public -p 6006 -c .storybook",
    "storybook-build": "build-storybook -s ./public -c .storybook -o build",
    "apollo:generate": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes=**/*.tsx --endpoint http://localhost:7000/graphql --tagName=gql --outputFlat app/types/generated"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "node_modules/.bin/eslint './{src,pages,server,app}/**/*' --ext .tsx,.ts --max-warnings=0",
      "git commit"
    ]
  },
  "husky": {
    "hooks": {}
  },
